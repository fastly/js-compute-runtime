{
  "scripts": {
    "install:ci": "HUSKY_SKIP_INSTALL=true npm ci",
    "build:test": "base=$npm_config_test; webpack \"./fixtures/$base/$base.ts\" --output-path \"./fixtures/$base\" --output-filename \"$base.js\"; js-compute-runtime --skip-pkg \"./fixtures/$base/$base.js\" \"./fixtures/$base/$base.wasm\"",
    "lint": "npx prettier --write ./fixtures/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.3",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@fastly/js-compute": "file:../../sdk/js-compute"
  }
}
