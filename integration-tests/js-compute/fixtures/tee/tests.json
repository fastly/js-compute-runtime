{
  "POST /": {
    "environments": ["viceroy", "c@e"],
    "downstream_request": {
      "method": "POST",
      "pathname": "/tee",
      "headers": [
        "Content-Type", "application/json"
      ],
      "body": "hello world!"
    },
    "downstream_response": {
      "status": 200,
      "body": "hello world!"
    }
  },

  "GET /error": {
    "environments": ["viceroy", "c@e"],
    "downstream_request": {
      "method": "GET",
      "pathname": "/error",
      "headers": [
          "Content-Type", "application/json"
      ]
    },
    "downstream_response": {
      "status": 200,
      "body": "Error: TypeError"
    }
  }
}
