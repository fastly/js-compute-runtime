{
  "name": "@fastly/js-compute",
  "version": "0.4.0",
  "js-compute-runtime-version": "0.4.0",
  "engines": {
    "node": "^16 || ^18"
  },
  "type": "module",
  "license": "Apache-2.0",
  "main": "js-compute-runtime-cli.js",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastly/js-compute-runtime"
  },
  "bin": {
    "js-compute-runtime": "js-compute-runtime-cli.js"
  },
  "files": [
    "bin",
    "reference-docs",
    "js-compute-runtime-cli.js",
    "index.d.ts",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "install:ci": "npm ci",
    "test": "tsd",
    "docs": "npm run docs:clean && npm run docs:typedoc && npm run docs:build-compute-app",
    "docs:clean": "rm -rf reference-docs compute-js",
    "docs:typedoc": "typedoc --theme ./node_modules/typedoc-loopingz-theme/bin/default  --disableSources --includeVersion --out reference-docs --tsconfig tsconfig.json --readme none --exclude index.test-d.ts index.d.ts",
    "docs:build-compute-app": "compute-js-static-publish --public-dir=./reference-docs --name js-compute-reference-docs --service-id 1B3wrEgiLdTaCjsmK5Jq25 --description '@fastly/js-compute reference documentation'",
    "postpublish": "npm run docs && cd ./compute-js && fastly compute publish"
  },
  "devDependencies": {
    "@fastly/compute-js-static-publish": "^2.0.0",
    "tsd": "^0.22.0",
    "typedoc": "^0.21",
    "typedoc-loopingz-theme": "^1.1.3",
    "typescript": "^4.2"
  }
}
