{
  "name": "@fastly/js-compute",
  "version": "0.5.6",
  "engines": {
    "node": "^16 || ^18"
  },
  "license": "Apache-2.0",
  "main": "js-compute-runtime-cli.js",
  "types": "index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastly/js-compute-runtime"
  },
  "bin": {
    "js-compute-runtime": "js-compute-runtime-cli.js"
  },
  "files": [
    "reference-docs",
    "js-compute-runtime-cli.js",
    "js-compute-runtime.wasm",
    "src",
    "index.d.ts",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "npm run test:types && npm run test:cli",
    "test:cli": "brittle --bail integration-tests/cli/**.test.js",
    "test:types": "tsd",
    "docs": "npm run docs:clean && npm run docs:typedoc",
    "docs:clean": "rm -rf reference-docs",
    "docs:typedoc": "typedoc --theme ./node_modules/typedoc-loopingz-theme/bin/default  --disableSources --includeVersion --out reference-docs --tsconfig tsconfig.json --readme none --exclude index.test-d.ts index.d.ts",
    "build": "make -C ./c-dependencies/js-compute-runtime && cp ./c-dependencies/js-compute-runtime/js-compute-runtime.wasm ."
  },
  "devDependencies": {
    "@jakechampion/cli-testing-library": "^1.0.0",
    "brittle": "^3.1.1",
    "eslint": "^8.27.0",
    "get-bin-path": "^7.2.1",
    "tsd": "^0.24.1",
    "typedoc": "^0.21",
    "typedoc-loopingz-theme": "^1.1.3",
    "typescript": "^4.2"
  },
  "dependencies": {
    "@jakechampion/wizer": "^1.5.3",
    "tree-sitter": "^0.20.1",
    "tree-sitter-javascript": "^0.19.0"
  }
}
