{
  "name": "@fastly/js-compute",
  "version": "0.5.8",
  "engines": {
    "node": "^16 || ^18"
  },
  "license": "Apache-2.0",
  "main": "js-compute-runtime-cli.js",
  "types": "types/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/fastly/js-compute-runtime"
  },
  "bin": {
    "js-compute-runtime": "js-compute-runtime-cli.js"
  },
  "files": [
    "types",
    "reference-docs",
    "js-compute-runtime-cli.js",
    "js-compute-runtime.wasm",
    "src",
    "index.d.ts",
    "package.json",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "test": "npm run test:types && npm run test:cli",
    "test:cli": "brittle --bail integration-tests/cli/**.test.js",
    "test:types": "tsd",
    "docs": "patch-package && npm run docs:clean && npm run docs:typedoc",
    "docs:clean": "rm -rf reference-docs compute-js",
    "docs:typedoc": "typedoc",
    "docs:deploy": "compute-js-static-publish  --public-dir=./reference-docs --name js-compute-reference-docs --service-id 1B3wrEgiLdTaCjsmK5Jq25 --description '@fastly/js-compute reference documentation' && cd ./compute-js && fastly compute publish",
    "build": "make -C ./c-dependencies/js-compute-runtime && cp ./c-dependencies/js-compute-runtime/js-compute-runtime.wasm ."
  },
  "devDependencies": {
    "@fastly/compute-js-static-publish": "^2.2.2",
    "@jakechampion/cli-testing-library": "^1.0.0",
    "brittle": "^3.1.1",
    "eslint": "^8.28.0",
    "get-bin-path": "^7.2.1",
    "patch-package": "^6.5.0",
    "tsd": "^0.24.1",
    "typedoc": "^0.23",
    "typedoc-loopingz-theme": "^1.1.3",
    "typedoc-plugin-mdn-links": "^2.0.0",
    "typedoc-plugin-versions": "^0.2.1",
    "typescript": "^4.9"
  },
  "dependencies": {
    "@jakechampion/wizer": "^1.5.3",
    "tree-sitter": "^0.20.1",
    "tree-sitter-javascript": "^0.19.0"
  }
}
